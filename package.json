{
  "name": "signal-form",
  "license": "MIT",
  "version": "0.7.4",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/remix.d.ts"
    },
    "./remix": {
      "require": "./dist/remix.cjs",
      "import": "./dist/remix.js",
      "types": "./dist/remix.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "test": "vitest run",
    "test:storybook": "concurrently -k -s first -n 'SB,TEST' -c 'magenta,blue' 'http-server storybook-static --port 16006' 'wait-on tcp:127.0.0.1:16006 && test-storybook --url http://127.0.0.1:16006'",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "check": "tsc",
    "release": "npm run check && npm run build && changeset publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "css-check": "ts-node --esm ./scripts/css-check.ts"
  },
  "dependencies": {
    "react": "^18.0.0",
    "signals-react-safe": "^1.1.2",
    "yup": "^1.3.2"
  },
  "optionalDependencies": {
    "@remix-run/react": ">=1.19.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@remix-run/testing": ">=1.19.0",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/blocks": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-vite": "^8.0.5",
    "@storybook/test": "^8.0.5",
    "@storybook/test-runner": "^0.18.1",
    "@types/inquirer": "^9.0.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/recursive-readdir": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^3.0.1",
    "chalk": "^5.2.0",
    "concurrently": "^8.2.2",
    "esbuild-css-modules-plugin": "^2.7.1",
    "eslint": "^8.53.0",
    "fetch-lite": "^1.1.0",
    "http-server": "^14.1.1",
    "inquirer": "^9.1.4",
    "kolorist": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "react-dom": "^18.2.0",
    "recursive-readdir": "^2.2.3",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.2",
    "type-fest": "^3.5.4",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^0.28.4",
    "wait-on": "^7.2.0"
  }
}
